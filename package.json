{
  "name": "boats",
  "version": "0.0.10",
  "description": "Beautiful Open Api Template System - Write less yaml with BOATS and Nunjucks.",
  "keywords": [
    "swagger",
    "openapi",
    "openapi2",
    "openapi3",
    "yaml",
    "bundle",
    "nunjucks",
    "yml",
    "yaml",
    "merge yml",
    "merge yaml",
    "DDD",
    "Documentation-Driven Development"
  ],
  "main": "index.js",
  "bin": {
    "boats": "./bin/cli.js"
  },
  "scripts": {
    "install:githooks": "cp ./githooks/* .git/hooks",
    "build:es5": "./node_modules/.bin/babel es6 -d es5",
    "build:readme:toc": "node ./node_modules/doctoc/doctoc.js .",
    "lint": "eslint *.js es6/",
    "test": "npm run test:build-run:oa2 && npm run test:build-run:oa3 && npm run test:unit",
    "test:unit": "NODE_ENV=TEST jest",
    "test:build-run:oa2": "npm run build:es5 && node exampleUseOA2.js",
    "test:build-run:oa3": "npm run build:es5 && node exampleUseOA3.js",
    "prepublish": "npm run build:readme:toc && npm run test && npm run build:es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johndcarmichael/boats.git"
  },
  "author": "John Carmichael",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johndcarmichael/boats/issues"
  },
  "homepage": "https://github.com/johndcarmichael/boats#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "commander": "^2.20.0",
    "dotenv": "^7.0.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "js-yaml": "^3.13.1",
    "json-refs": "^3.0.13",
    "nunjucks": "^3.2.0",
    "swagger-parser": "^6.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "doctoc": "^1.4.0",
    "eslint": "^5.16.0",
    "jest": "^23.6.0"
  }
}
